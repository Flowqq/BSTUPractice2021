<Window x:Class="Program.SearchWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Program"
        mc:Ignorable="d"
        Title="SearchWindow" Height="700" Width="600">
    <Grid>
        <TextBox x:Name="NameTextBox" HorizontalAlignment="Left" Margin="25,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="179" Height="28"/>
        <ListBox x:Name="ResultListBox" Margin="10,91,10,10"/>
        <TextBox x:Name="ValueTextBox" HorizontalAlignment="Left" Margin="234,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="179" Height="28"/>
        <TextBlock HorizontalAlignment="Left" Margin="25,23,0,0" Text="Name" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="234,23,0,0" Text="Value" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <Button Content="Search" HorizontalAlignment="Left" Margin="457,44,0,0" VerticalAlignment="Top" Height="28" Width="97" Click="SearchButtonClick"/>

    </Grid>
</Window>
